<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proteintracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProteinTracker">
        <activity
            android:name=".SampleListViewActivity"
            android:exported="false" />
        <activity
            android:name=".SampleListActivity"
            android:exported="true"
            android:label="Halaman Sample List">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProteinTrackerActivity"
            android:exported="true"></activity>
        <activity
            android:name=".TableActivity"
            android:exported="true"
            android:label="Table Activity"></activity>
        <activity
            android:name=".ConstraintActivity"
            android:exported="true"
            android:label="Constraint Activity" />
        <activity
            android:name=".LinearActivity"
            android:exported="true"
            android:label="Linear Activity" />
        <activity
            android:name=".HelpActivity"
            android:exported="true"
            android:label="Help Activity" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="Main Activity" />
    </application>

</manifest>